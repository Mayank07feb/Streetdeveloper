    <span class="material-icons text-blue-500 align-middle text-7xl sm:text-8xl md:text-9xl mr-4">public</span>
    Running Projects Across Countries
  </h2>

  <!-- World Map Chart -->
  <div id="chartdiv" class="rounded-lg overflow-hidden shadow-lg"></div>

  <!-- Styles -->
  <style>
    #chartdiv {
      width: 100%;
      height: 600px;
    }

    @media (max-width: 768px) {
      #chartdiv {
        height: 400px;
      }
    }

    @media (max-width: 480px) {
      #chartdiv {
        height: 300px;
      }
    }
  </style>

  <!-- AmCharts Resources -->
  <script src="https://cdn.amcharts.com/lib/5/index.js"></script>
  <script src="https://cdn.amcharts.com/lib/5/map.js"></script>
  <script src="https://cdn.amcharts.com/lib/5/geodata/worldLow.js"></script>
  <script src="https://cdn.amcharts.com/lib/5/themes/Animated.js"></script>

  <!-- Chart Code -->
  <script>
    am5.ready(function () {

      // Sample data for countries
      var data = [
        { id: "US", name: "United States", value: 100 },
        { id: "GB", name: "United Kingdom", value: 100 },
        { id: "CN", name: "China", value: 100 },
        { id: "IN", name: "India", value: 100 },
        { id: "AU", name: "Australia", value: 100 },
        { id: "CA", name: "Canada", value: 100 },
        { id: "BR", name: "Brasil", value: 100 },
        { id: "ZA", name: "South Africa", value: 100 }
      ];

      // Create root element
      var root = am5.Root.new("chartdiv");
      root.setThemes([am5themes_Animated.new(root)]);

      // Create the map chart with zoom and pan disabled
      var chart = root.container.children.push(am5map.MapChart.new(root, {
        panX: false,          // Disable horizontal dragging
        panY: false,          // Disable vertical dragging
        wheelX: "none",       // Disable horizontal wheel zoom
        wheelY: "none",       // Disable vertical wheel zoom
        pinchZoom: false       // Disable pinch zoom on touch devices
      }));

      // Polygon series for countries
      var polygonSeries = chart.series.push(
        am5map.MapPolygonSeries.new(root, {
          geoJSON: am5geodata_worldLow,
          exclude: ["AQ"]  // Exclude Antarctica
        })
      );

      // Bubble series for country markers
      var bubbleSeries = chart.series.push(
        am5map.MapPointSeries.new(root, {
          valueField: "value",
          calculateAggregates: true,
          polygonIdField: "id"
        })
      );

      // Circle template for bubbles
      var circleTemplate = am5.Template.new({});

      // Add bubbles and country labels
      bubbleSeries.bullets.push(function (root, series, dataItem) {
        var container = am5.Container.new(root, {});

        var circle = container.children.push(
          am5.Circle.new(root, {
            radius: 20,
            fillOpacity: 0.7,
            fill: am5.color(0xff0000),
            cursorOverStyle: "pointer",
            tooltipText: `{name}: [bold]{value}[/]`
          }, circleTemplate)
        );

        var countryLabel = container.children.push(
          am5.Label.new(root, {
            text: "{name}",
            paddingLeft: 5,
            populateText: true,
            fontWeight: "bold",
            fontSize: 13,
            centerY: am5.p50
          })
        );

        circle.on("radius", function (radius) {
          countryLabel.set("x", radius);
        });

        return am5.Bullet.new(root, {
          sprite: container,
          dynamic: true
        });
      });

      // Show bubble values
      bubbleSeries.bullets.push(function (root, series, dataItem) {
        return am5.Bullet.new(root, {
          sprite: am5.Label.new(root, {
            text: "{value.formatNumber('#.')}",
            fill: am5.color(0xffffff),
            populateText: true,
            centerX: am5.p50,
            centerY: am5.p50,
            textAlign: "center"
          }),
          dynamic: true
        });
      });

      // Heat rules for bubble size
      bubbleSeries.set("heatRules", [
        {
          target: circleTemplate,
          dataField: "value",
          min: 10,
          max: 50,
          minValue: 0,
          maxValue: 100,
          key: "radius"
        }
      ]);

      // Set initial data
      bubbleSeries.data.setAll(data);

      // Function to update data dynamically every 2 seconds
      updateData();
      setInterval(function () {
        updateData();
      }, 2000);

      function updateData() {
        for (var i = 0; i < bubbleSeries.dataItems.length; i++) {
          bubbleSeries.data.setIndex(i, {
            value: Math.round(Math.random() * 100),
            id: data[i].id,
            name: data[i].name
          });
        }
      }

    });
  </script>
